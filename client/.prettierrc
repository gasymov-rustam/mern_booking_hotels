{
  "singleQuote": true,
  "arrowParens": "always",
  "printWidth": 120,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": ["^components/(.*)$", "^../(.*)$", "^\\./(.*)$", "^(.)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
